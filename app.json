{
  "expo": {
    "name": "VateLanka",
    "slug": "VateLanka",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCAlQrLUYEmcVRj8nd1W6HZKOpMfHsweDw"
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nemitha2005.VateLanka",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCAlQrLUYEmcVRj8nd1W6HZKOpMfHsweDw"
        }
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification_icon.png",
          "color": "#00B386",
          "sounds": [
            "./assets/notification_sound.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8da7a432-6d74-4fb3-ac24-b8338840033b"
      }
    }
  }
}